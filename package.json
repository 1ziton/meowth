{
  "name": "meowth",
  "version": "1.0.0",
  "description": "智能化DevOps辅助系统",
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start_prod": "npm-run-all --parallel start_prod:*",
    "start_prod:client": "cd client && npm run start:prod",
    "start_prod:server": "cd server && npm run start:prod",
    "start:client": "cd client && npm run start",
    "start:server": "cd server && npm run start",
    "client:build": "cd client && npm run build",
    "server:build": "cd server && npm run build",
    "doc": "apidoc -i server/src/controller -o doc/apidoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wufengliang/meowth.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wufengliang/meowth/issues"
  },
  "homepage": "https://github.com/wufengliang/meowth#readme",
  "dependencies": {
    "apidoc": "^0.17.7"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
